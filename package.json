{
  "name": "psuedolinux_backend",
  "version": "1.0.0",
  "description": "Production-ready secure GET-only backend for VRChat integration",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "test": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"test/**/*.ts\"",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d src/infra/database/data-source.ts",
    "db:migrate:revert": "typeorm-ts-node-commonjs migration:revert -d src/infra/database/data-source.ts",
    "db:generate": "typeorm-ts-node-commonjs migration:generate -d src/infra/database/data-source.ts"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.629.0",
    "@aws-sdk/client-secrets-manager": "^3.629.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/typeorm": "^10.0.0",
    "@types/express": "^4.17.21",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "argon2": "^0.31.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.2",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "nestjs-pino": "^4.0.0",
    "pino": "^8.16.2",
    "pino-http": "^10.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "pg": "^8.11.3",
    "compression": "^1.7.4",
    "zod": "^3.22.4",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.7.0",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "@types/compression": "^1.7.5",
    "pino-pretty": "^10.3.1",
    "sqlite3": "^5.1.7",
    "@nestjs/testing": "^11.1.6"
  }
}
