NODE_ENV=development
APP_PORT=8080
APP_GLOBAL_PREFIX=api
APP_BODY_LIMIT=10kb
AWS_REGION=us-east-1
AWS_SECRETS_MANAGER_SECRET_ID=your-secrets-id
AWS_KMS_KEY_ID=your-kms-key
CRYPTO_INFO_HMAC=v1/hmac
CRYPTO_INFO_AEAD=v1/aead
CRYPTO_KEY_GRACE=86400
CRYPTO_ALLOWED_VERSIONS=1
SECURITY_MAX_TTL=300
SECURITY_CLOCK_SKEW=10
SECURITY_NONCE_PADDING=5
SECURITY_CANONICALIZE_SLASH=true
REDIS_HOST=redis.example.com
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_TLS=false
REDIS_KEY_PREFIX=vrchat
DB_HOST=db.example.com
DB_PORT=5432
DB_USERNAME=vrchat
DB_PASSWORD=secure-password
DB_DATABASE=vrchat
DB_SSL=true
CRYPTO_MASTER_SECRET_OVERRIDE=
